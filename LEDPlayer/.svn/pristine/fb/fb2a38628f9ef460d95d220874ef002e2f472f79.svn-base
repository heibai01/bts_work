package com.led.player.vo;
/**
 * 对应一天的开关瓶设置
 * 一天最多有三个此实例对象。。什么玩意啊。
 * @author 1231
 *
 */
public class OnOffScrnDayVo {
	public byte isOn; //0停用，1启用。
	public String onTime; //hh:mm格式 2B
	public String offTime; //同上
	
	public OnOffScrnConfigVo onOffScrnConfigVo;

	@Override
	public String toString() {
		return "OnOffScrnDayVo [isOn=" + isOn + ", onTime=" + onTime
				+ ", offTime=" + offTime + ", onOffScrnConfigVo="
				+ onOffScrnConfigVo + "]";
	}
	
	
	
}
